node("x86_runner1"){
    stage('*** Cleanup***') {
        cleanWs()
    }
    stage('*** Prepare Environment ***') {
        sh '''#!/bin/bash
            ls
            rm -rf ./*
         '''
    } 
    stage('*** Check out SCM ***') {
        checkout scm
    } 
    stage('*** Compilation Phase ***') { 
        sh '''#!/bin/bash
            cat ./jenkinsfile
            as --version
         '''
    }
    stage (' *** Test ***'){
        sh '''#!/bin/bash
            ls
         '''
    }
}
