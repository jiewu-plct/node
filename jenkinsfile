node("x86_runner1"){
    stage('*** Cleanup***') {
        cleanWs()
    }
    stage('*** Prepare Environment ***') {
        sh '''#!/bin/bash
            ls
            rm -rf ./*
         '''
    } 
    stage('*** Check out SCM ***') {
        checkout scm
    } 
    stage('*** Compilation Phase ***') { 
        sh '''#!/bin/bash
            cat ./jenkinsfile
            as --version
            strings /lib/x86_64-linux-gnu/libc.so.6 |grep GLIBC_
         '''
    }
    stage (' *** Test ***'){
        sh '''#!/bin/bash
            ls
         '''
    }
}
