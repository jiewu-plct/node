node("sf2-3"){
    stage('*** Cleanup***') {
        cleanWs()
    }
    stage('*** Check out SCM ***') {
        checkout scm
    } 
    stage('*** Compilation Phase ***') { 
        sh '''#!/bin/bash
            ./configure --openssl-no-asm
            make -j $(nproc)
         '''
    }
    stage (' *** Test ***'){
        sh '''#!/bin/bash
            ./node -v
         '''
    }
}
